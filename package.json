{
  "name": "pestcontrol-reporting-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "powershell -ExecutionPolicy Bypass -File ../free-ports.ps1 -VerboseOutput",
    "dev": "npm run predev && nodemon --watch src --ext ts --exec \"npm run start:ts\"",
    "dev:node": "npm run predev && npm run build && node dist/server.js",
    "start:ts": "tsc -p . && node dist/server.js",
    "build": "tsc -p .",
    "start": "node dist/server.js",
    "dev:both": "powershell -File ../start-dev.ps1"
  },
  "dependencies": {
    "@prisma/client": "5.22.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.7",
    "nodemailer": "^6.9.8",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.11.30",
    "@types/nodemailer": "^7.0.4",
    "nodemon": "^3.1.11",
    "prisma": "5.22.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.0"
  },
  "prisma": {
    "seed": "node --loader ts-node/esm prisma/seed.ts"
  }
}
